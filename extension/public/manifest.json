{
  "manifest_version": 3,
  "name": "Strapi Summarize",
  "description": "Strapi Powered Open AI Extension to summarize web articles.",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "scripting",
    "alarms",
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "action": {
    "default_popup": "index.html"
  }
}